
local Framework = loadstring(game:HttpGet("https://raw.githubusercontent.com/Thaytubervn/ThaytuberVN-Menu/refs/heads/main/ThaytuberVN%20Hub"))()


--!strict

-- Giả sử bạn đã gọi từ framework: local Framework = require(path)
local Window = Framework:CreateWindow({
    Name = "My Menu",
    ToggleUIKeybind = "K"
})

-- Tạo tab chính
local Tab1 = Window:CreateTab("Main")

-- Tạo nút trong tab chính
Tab1:CreateButton({
    Name = "Print Hello",
    Callback = function()
        print("Hello")
    end
})

-- Bật hiệu ứng viền RGB
Window:ToggleRGB(true)

-- Có thể thêm nhiều tab
local Tab2 = Window:CreateTab("Player")

Tab2:CreateButton({
    Name = "WalkSpeed x2",
    Callback = function()
        local char = game.Players.LocalPlayer.Character
        if char and char:FindFirstChildOfClass("Humanoid") then
            char:FindFirstChildOfClass("Humanoid").WalkSpeed = 32
        end
    end
})

-- Thêm tab Setting mặc định
local SettingTab = Window:CreateTab("Setting")
SettingTab:CreateButton({
    Name = "Toggle Rainbow Border",
    Callback = function()
        Window:ToggleRGB()
    end
})
